{
  <% @top.each do |attribute| %>
  "<%= attribute[0] %>": "<%= attribute[1] %>",
  <% end %>
  "pcf": {
    <% @pcf.each do |attribute| %>
      "<%= attribute[0].delete_prefix("pcf_") %>": "<%= attribute[1] %>",
    <% end %>
    "dqi": {
      <% @dqi.each do |attribute| %>
        "<%= attribute[0].delete_prefix("pcf_dqi_") %>": "<%= attribute[1] %>",
      <% end %>
    },
    "assurance": {
      <% @assurance.each do |attribute| %>
        "<%= attribute[0].delete_prefix("pcf_assurance_") %>": "<%= attribute[1] %>",
      <% end %>
    }
  }
}